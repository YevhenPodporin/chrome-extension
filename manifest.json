{
	"name": "replacement-dictionary ",
	"description": "test-task",
	"version": "1.0",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"run_at": "document_idle",
			"js": [
				"index.js"
			],
			"css": [
				"style.css"
			],
			"all_frames": true
		}
	],
	"icons": {
		"32": "img/icon-32.png",
		"64": "img/icon-64.png"
	},
	"permissions": [
		"storage"
	]
}